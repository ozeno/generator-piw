        "babel-preset-es2015": "^6.24.1",
        "clean-webpack-plugin": "^0.1.19",
        "extract-text-webpack-plugin": "^2.1.2",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
